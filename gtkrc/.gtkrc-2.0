include "/home/rodesp/.gtkrc-2.0.mine"
gtk-theme-name="Everforest-Dark"
gtk-icon-theme-name="Everforest-Dark"
gtk-font-name="FiraCode Nerd Font weight=450 12"
gtk-cursor-theme-name="vimix"
gtk-cursor-theme-size=24
